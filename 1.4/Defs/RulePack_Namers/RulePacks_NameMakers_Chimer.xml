<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RulePackDef>
		<defName>ESCP_NamerPersonChimerMale</defName>
		<include>
		  <li>TribalWordUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>name->[MaleList] [FamilyList]</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>MaleList</keyword>
					<path>ESCP_DunmerNames/DunmerMale</path>
				</li>
				<li Class="Rule_File">
					<keyword>FamilyList</keyword>
					<path>ESCP_DunmerNames/DunmerFamily</path>
				</li>
			</rulesRaw>
		</rulePack>
	</RulePackDef>

	<RulePackDef>
		<defName>ESCP_NamerPersonChimerFemale</defName>
		<include>
		  <li>TribalWordUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>name->[FemaleList] [FamilyList]</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>FemaleList</keyword>
					<path>ESCP_DunmerNames/DunmerFemale</path>
				</li>
				<li Class="Rule_File">
					<keyword>FamilyList</keyword>
					<path>ESCP_DunmerNames/DunmerFamily</path>
				</li>
			</rulesRaw>
		</rulePack>
	</RulePackDef>
	
</Defs>