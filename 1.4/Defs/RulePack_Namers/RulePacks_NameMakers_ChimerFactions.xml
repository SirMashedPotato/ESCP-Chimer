<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>ESCP_NamerSettlementChimerHouse</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name->[ChimerCities]</li>
		<li>r_name(p=0.1)->Shrine of [DunmerSaints]</li>
		<li>r_name(p=0.1)->Shrine of Saint [ChimerGods]</li>
      </rulesStrings>
	  <rulesFiles>
        <li>ChimerCities->ESCP_ChimerNames/ChimerCities</li>
		<li>DunmerSaints->ESCP_DunmerNames/DunmerSaints</li>
		<li>ChimerGods->ESCP_ChimerNames/ChimerGods</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>ESCP_NamerFactionChimer</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name->Chimer pilgrims of Saint [DunmerSaints]</li>
        <li>r_name->Chimer pilgrims of [ChimerGods]</li>
      </rulesStrings>
	  <rulesFiles>
	    <li>DunmerSaints->ESCP_DunmerNames/DunmerSaints</li>
		<li>ChimerGods->ESCP_ChimerNames/ChimerGods</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>

</Defs>
